{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
      "includeFiles": [
        "dist/**", 
        "views/**", 
        "generated/prisma/**" 
      ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}